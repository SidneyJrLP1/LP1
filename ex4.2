#include <stdio.h>
int eh_primo (int n) {
   int i;
   for (i = 2; i < n; i++) {
     if (n % i == 0) {
        printf ("%d não é primo\n", n);
        break;
     } else if ( i == (n - 1)) {
        printf ("É primo\n");
     }
   }
}
int main (void) {
   int N;
   scanf("%d", &N);
   eh_primo(N);
   return 0;
}  
